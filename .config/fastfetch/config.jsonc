{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "builtin",
        "source": "arch_small",
        "padding": {
            "top": 2,
            "left": 4
        },
        "color": {
            "1": "#f9b883",
            "2": "#ebdbb2"
        },
        "height": 12
    },
    "modules": [
        "break",
        {
