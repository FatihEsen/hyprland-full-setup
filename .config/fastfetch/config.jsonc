            "separator": "  "
        },
        {
            "type": "de",
            "key": "󰉼 DE",
            "keyColor": "#b8bb26",
            "format": "Hyprland",
            "separator": "  "
        },
        {
            "type": "wm",
            "key": "󰖲 WM",
            "keyColor": "#b8bb26",
            "format": "Hyprland",
            "separator": "  "
        },
        {
            "type": "terminal",
            "key": " Term",
            "keyColor": "#d3869b",
            "separator": "  "
        },
        {
            "type": "shell",
            "key": " Shell",
            "keyColor": "#fb4934",
            "separator": "  "
